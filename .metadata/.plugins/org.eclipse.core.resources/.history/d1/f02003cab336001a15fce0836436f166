package comm;

public class TestingExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub  
			  
		    int findMax(int arr[]){  
		        int max=0;  
		        for(int i=1;i<arr.length;i++){  
		            if(max<arr[i])  
		                max=arr[i];  
		        }  
		        return;  
		    }  

	}

}
